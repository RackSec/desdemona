expr ::= eq | fn-call | ipv4-address | identifier

fn-call ::= identifier <"("> identifier <")">
identifier ::= #"[A-Za-z-]+"

maybe-whitespace ::= " "*

eq ::= identifier <maybe-whitespace> <"="> <maybe-whitespace> identifier

ipv4-address ::= decimal-byte <"."> decimal-byte <"."> decimal-byte <"."> decimal-byte
<decimal-byte> ::= #"25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?"
